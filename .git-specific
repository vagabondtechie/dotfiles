alias gst="git status"
alias gsh="git stash"
alias gshs="git stash save"
alias gpl="git pull --rebase origin master"
alias ga="git add -A"
alias gac="git add -A && git commit"
alias gacn="git add -A && git commit --amend --no-edit"
alias gps="git push origin master"
alias gpsg="git push origin HEAD:refs/for/master"
alias gd="git diff"
alias gdl="git diff HEAD^ HEAD"
alias gs="git show"
alias gsf="git show --name-only"
alias gl="git log"
alias gll="git log -1"
